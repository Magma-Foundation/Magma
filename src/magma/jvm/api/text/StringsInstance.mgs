export interface StringsInstance {
	def length(stripped: &[I8]): I32;

	def sliceBetween(input: &[I8], startInclusive: I32, endExclusive: I32): &[I8];

	def sliceFrom(input: &[I8], startInclusive: I32): &[I8];

	def isEmpty(cache: &[I8]): Bool;

	def equalsTo(left: &[I8], right: &[I8]): Bool;

	def strip(input: &[I8]): &[I8];

	def isBlank(value: &[I8]): Bool;

	def charAt(input: &[I8], index: I32): I8;

}
