import magma.app.compile.Caller;
import magma.app.Platform;
import magma.app.Value;
import magma.api.option.Option;
import magma.api.option.None;
struct ConstructionCaller implements Caller {
	mut right: &[I8];
	mut platform: Platform;
	constructor (mut right: &[I8], mut platform: Platform) {
		this.right = right;
		this.platform = platform;
	}
	def generate(): &[I8] {
		if (Platform.Magma === this.platform) {
			return this.right;
		}
		return "new " + this.right;
	}
	def findChild(): Option<Value> {
		return None<Value>();
	}
}
