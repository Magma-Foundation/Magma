	def static foldDoubleQuotes(state: DivideState, c: I8): Option<DivideState> {
		if ("\"" !== c) {
			return None<DivideState>();
		}
		let mut appended: DivideState = state.append(c);
		while (true) {
			let maybeTuple: Tuple2<Bool, Tuple2<DivideState, I8>> = appended.popAndAppendToTuple().toTuple(Tuple2Impl<DivideState, I8>(appended, "\0"));
			if (!maybeTuple.left()) {
				break;
			}
			let tuple: Tuple2<DivideState, I8> = maybeTuple.right();
			appended = tuple.left();
			if ("\\" === tuple.right()) {
				appended = appended.popAndAppendToOption().orElse(appended);
			}
			if ("\"" === tuple.right()) {
				break;
			}
		}
		return Some<DivideState>(appended);
	}
	def static foldSingleQuotes(state: DivideState, c: I8): Option<DivideState> {
