struct Import {
	namespace: List<&[I8]>;
	child: &[I8];
	constructor (namespace: List<&[I8]>, child: &[I8]) {
		this.namespace = namespace;
		this.child = child;
	}
	def generate(platform: Platform): &[I8] {
		if (Platform.Magma === platform){
			let joinedNamespace = this.namespace.query().collect(Joiner(".")).orElse("");
			return "import " + joinedNamespace + "." + this.child + ";\n";
		}
		let joinedNamespace = this.namespace.addLast(this.child).query().collect(Joiner("/")).orElse("");
		return "import { " + this.child + " } from \"" + joinedNamespace + "\";\n";
	}
