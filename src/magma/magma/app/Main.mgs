	}
	def joinTypeParams(): &[I8] {
		return Main.joinTypeParams(this.typeParams);
	}
	def hasAnnotation(annotation: &[I8]): boolean {
		return this.annotations.contains(annotation, Strings.equalsTo);
	}
	def removeModifier(modifier: &[I8]): MethodHeader {
		return Definition(this.annotations, this.modifiers.removeValue(modifier, Strings.equalsTo), this.typeParams, this.type, this.name);
	}
	def addModifier(modifier: &[I8]): MethodHeader {
		return Definition(this.annotations, this.modifiers.addLast(modifier), this.typeParams, this.type, this.name);
	}
}
