import magma.api.option.None;import magma.api.option.Option;import magma.api.option.Some;import java.util.function.Function;export class def Ok<T, E>(value : T) => {public static def from<E, R>(this, value : R) : Result<R, E> => {return Ok(value);}public def flatMapValue<R>(this, mapper : (T) => Result<R, E>) : Result<R, E> => {return mapper.apply(value);}public def mapValue<R>(this, mapper : (T) => R) : Result<R, E> => {return Ok(mapper.apply(value));}public def isOk(this) : boolean => {return true;}public def mapErr<R>(this, mapper : (E) => R) : Result<T, R> => {return Ok(value);}public def match<R>(this, onOk : (T) => R, onErr : (E) => R) : R => {return onOk.apply(value);}public def findValue(this) : Option<T> => {return Some(value);}public def findErr(this) : Option<E> => {return None();}}