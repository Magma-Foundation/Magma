
import java.util.function.Function;
import java.util.function.Supplier;
export class def Some<T>() => {
	private final value : T;
	def Some(this, value : T) : public => {
		this=value;
	}
	public def map<R>(this, mapper : (T) => R) : Option<R> => {
		return Some(mapper.apply(value));
	}
	public def orElseGet(this, supplier : Supplier<T>) : T => {
		return value;
	}
}
