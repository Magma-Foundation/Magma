import java.util.function.Predicate;
() => {
	public foldLeftToResult<R, E>(initial : R, mapper : BiFunction<R, T, Result<R, E>>) => this.foldLeft(Ok.from(initial), () => reResult.flatMapValue(() => mapper.apply(inner, t)));
	public anyMatch(predicate : Predicate<T>) => foldLeft(false, () => aBoolean||predicate.test(t));
	public filter(filter : Predicate<T>) => flatMap(() => HeadedStream(filter.test(value)?SingleHead(value):EmptyHead.EmptyHead()));
	public flatMap<R>(mapper : Function<T, Head<R>>) => HeadedStream(head().map(mapper).map(() => FlatMapHead(initial, this, mapper)).orElse(EmptyHead.EmptyHead()));
	public extend<R>(mapper : Function<T, R>) => map(() => Tuple(value, mapper.apply(value)));
	public map<R>(mapper : Function<T, R>) => HeadedStream(() => this.provider.head().map(mapper));
	public collect<C>(collector : Collector<T, C>) => {
		
		current : var=
		
		collector.createInitial();
		return foldLeft(current, collector.fold);
	}
	public foldLeft<C>(current : C, folder : BiFunction<C, T, C>) => while(true){
		
		finalCurrent : C=
		
		current;
		
		tuple : var=
		
		JavaOptionals.toTuple(JavaOptionals.toNative(head()).map(() => folder.apply(finalCurrent, head)), current);
		if
		(tuple.left())
		{
			current=tuple.right();
		}
		else
		{
			return current;
		}
	}
	public head() => provider.head();
	default class def FlatMapHead() => {
		private final outer : Head<T>;
		private final mapper : Function<T, Head<R>>;
		private current : Head<R>;
		FlatMapHead(initial : Head<R>, outer : HeadedStream<T>, mapper : Function<T, Head<R>>) => {
			this=outer;
			this=mapper;
			current=initial;
		}
		public head() => while(true){
			
			currentHead : var=
			
			current.head();
			if
			(currentHead.isPresent())
			return currentHead;
			
			tuple : var=
			
			outer.head().map(mapper).toTuple(current);
			if
			(tuple.left())
			{
				current=tuple.right();
			}
			else
			{
				return None.None();
			}
		}
	}
}