import magma.api.option.None;
import magma.api.option.Option;
import magma.api.option.Some;
export default class def SingleHead() => {
	private final value : T;
	
	private retrieved : boolean=
	
	false;
	SingleHead(value : T) => this=value;
	public def head() => {
		if
		(retrieved)
		return None.None();
		else
		{
			retrieved=true;
			return Some(value);
		}
	}
}