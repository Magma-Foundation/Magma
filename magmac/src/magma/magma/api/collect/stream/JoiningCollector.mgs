import java.util.Optional;
export default class def JoiningCollector(delimiter : String) => {
	public def createInitial(this : Self) => Optional.empty();
	public def fold(this : Self, current : Optional<String>, next : String) => current.isEmpty()?Optional.of(next):current.map(() => inner+delimiter+next);
	implements Collector<String, Optional<String>>;
}