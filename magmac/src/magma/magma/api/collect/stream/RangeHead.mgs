import magma.api.option.None;
import magma.api.option.Option;
import magma.api.option.Some;
export object RangeHead {
	private final extent : int;
	private counter : int;
	public def head() => {
		if
		(counter>=extent)
		return None.None();
		
		value : var=
		
		counter;
		counter++;
		return Some(value);
	}
	implements Head<Integer>;
}