import java.util.Optional;
import java.util.function.Function;
export struct Result<T, E> {
	findValue() : Optional<T>;
	findErr() : Optional<E>;
	flatMapValue(mapper : T => Result<R, E>) : Result<R, E>;
	mapValue(mapper : T => R) : Result<R, E>;
	isOk() : boolean;
	mapErr(mapper : E => R) : Result<T, R>;
	match(onOk : T => R, onErr : E => R) : R;
}
