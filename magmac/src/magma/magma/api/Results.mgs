
public class def Results() => {
	public static def unwrap<T, E extends Throwable>(result : Result<T, E>) : T => {
		value = result.findValue();
		if
		(value.isPresent())
		return value.get();
		err = result.findErr();
		if
		(err.isPresent())
		throw err.get();
		throw RuntimeException("No value or err present?");
	}
}
