
export class def Results() => {
	public static def unwrap<T, E extends Throwable>(this, result : Result<T, E>) : T => {
		let mut value = result.findValue();
		if(value.isPresent())
		return value.get();
		let mut err = result.findErr();
		if(err.isPresent())
		throw err.get();
		throw RuntimeException("No value or err present?");
	}
}
