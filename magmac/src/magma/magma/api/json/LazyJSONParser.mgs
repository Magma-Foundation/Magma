import magma.api.option.None;
import magma.api.option.Option;
import magma.api.option.Some;
export default class def LazyJSONParser() => {
	
	private value : Option<JSONParser>=
	
	None.None();
	public setValue(value : JSONParser) : void => {
		this=Some(value);
	}
	public parse(input : String) : Option<JSONValue> => {
		return value.flatMap(() => inner.parse(input));
	}
}