
import magma.api.result.Result;
export class def JavaResults() => {
	public static def unwrap<T, E extends Throwable>(this, result : Result<T, E>) : T => {
		let mut value = result.findValue();
		if(value.isPresent())
		{
			return value.orElseThrow();
		}
		let mut error = result.findErr();
		if(error.isPresent())
		{
			throw error.get();
		}
		throw RuntimeException("Neither a value nor an error is present. This result contains nothing.");
	}
}
