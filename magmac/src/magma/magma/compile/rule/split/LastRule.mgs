
import magma.compile.rule.Rule;
import magma.compile.rule.Rules;
import java.util.Optional;
public final class def LastRule() => {
	def LastRule(leftRule : Rule, slice : String, rightRule : Rule) : public => {
		super(leftRule, slice, rightRule);}
	protected def computeIndex(input : String) : Optional<Integer> => {
		return Rules.wrapIndex(input.lastIndexOf(slice));}}