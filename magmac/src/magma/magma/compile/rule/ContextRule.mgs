
import magma.api.Result;
import magma.compile.CompileParentError;
import magma.compile.Error_;
import magma.compile.rule.result.RuleResult;
export class def ContextRule(message : String, child : Rule) => {
	public def toNode(input : String) : RuleResult => {
		return child.toNode(input).mapErr(() => CompileParentError(message, input, err));
	}
	public def fromNode(node : Node) : Result<String, Error_> => {
		return child.fromNode(node);
	}
}
