export class def NumberRule() => {def NumberRule(this, child : Rule) : public => {super(child);}private static def allDigits(this, input : String) : boolean => {if(input.isEmpty())return false;let mut i : int = 0;while(i<input.length()){let mut c : var = input.charAt(i);if(!Character.isDigit(c)){return false;}i++;}return true;}protected def computeMessage(this) : String => {return "Not a number.";}protected def filter(this, input : String) : boolean => {return input.startsWith("-")?allDigits(input.substring(1)):allDigits(input);}}