import java.io.IOException;
export class def CompileException() => {
	def CompileException(this, message : String, cause : Throwable) : public => {
		super(message, cause);
	}
	def CompileException(this, message : String) : public => {
		super(message);
	}
	def CompileException(this) : public => {
	}
	def CompileException(this, cause : IOException) : public => {
		super(cause);
	}
}
