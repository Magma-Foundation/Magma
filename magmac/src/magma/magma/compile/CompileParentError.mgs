import java.util.Collections;
import java.util.List;
import java.util.Optional;
export default class def CompileParentError(message : String, context : String, parent : Error_) => {
	public def findMessage(this : Self) => Optional.of(message);
	public def findCauses(this : Self) => Optional.of(Collections.singletonList(parent));
	public def findContext(this : Self) => Optional.of(context);
	public def calculateDepth(this : Self) => 1+parent.calculateDepth();
	implements Error_;
}