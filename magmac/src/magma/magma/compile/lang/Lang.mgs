	def createParamsRule(this : Self, definition : Rule) => SplitMultipleRule(ParamSplitter(), ", ", "params", StripRule(TypeRule("definition", definition)));
	def createStringRule(this : Self) => TypeRule("string", LeftRule("\"", RightRule(ExtractStringRule("value"), "\"")));
	def createAccessRule(this : Self, type : String, separator : String, value : Rule) => {
		
