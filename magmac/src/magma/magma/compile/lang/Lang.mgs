		return SplitMultipleRule(ParamSplitter(), ", ", "type-params", typeParam);
	}
	public def createStatementRule(this : Self, value : LazyRule) => {
		
		child : var=
		
		ExtractNodeRule("child", StripRule(value));
		return TypeRule("statement", RightRule(child, ";"));
	}
}