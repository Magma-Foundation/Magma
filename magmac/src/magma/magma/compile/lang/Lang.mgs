	def createSymbolRule(this : Self) => TypeRule("symbol", SymbolRule(ExtractStringRule("value")));
	def createTernaryRule(this : Self, value : LazyRule) => TypeRule("ternary", FirstRule(StripRule(ExtractNodeRule("condition", value)), "?", FirstRule(StripRule(ExtractNodeRule("true", value)), ":", StripRule(ExtractNodeRule("false", value)))));
	def createOperatorRule(this : Self, name : String, slice : String, value : Rule) => {
		
		left : var=
		
		StripRule(ExtractNodeRule("leftRule", value));
		
		right : var=
		
		StripRule(ExtractNodeRule("right", value));
		return TypeRule(name, SplitOnceRule(left, slice, right, OperatorSearcher(slice)));
	}
	def createNumberRule(this : Self) => TypeRule("number", NumberRule(ExtractStringRule("value")));
	def createCharRule(this : Self) => TypeRule("char", LeftRule("'", RightRule(ExtractStringRule("value"), "'")));
	def createEmptyStatementRule(this : Self) => TypeRule("empty", RightRule(StripRule(EmptyRule("value")), ";"));
	def createThrowRule(this : Self, value : Rule) => {
		
		rule : Rule=
		
		ExtractNodeRule("value", value);
		
		after : var=
		
		RightRule(rule, ";");
		return TypeRule("throw", LeftRule("throw ", after));
	}
	def createNotRule(this : Self, value : LazyRule) => TypeRule("not", LeftRule("!", ExtractNodeRule("child", value)));
	def createDefinitionRule(this : Self, definition : Rule) => TypeRule("definition", RightRule(definition, ";"));
	def createBlockCommentRule(this : Self) => TypeRule("block-comment", StripRule(LeftRule("/*", RightRule(ExtractStringRule("value"), "*/"))));
	def createPostDecrementRule(this : Self, value : LazyRule) => TypeRule("post-decrement", RightRule(ExtractNodeRule("value", value), "--;"));
	def createPostIncrementRule(this : Self, value : LazyRule) => TypeRule("post-increment", RightRule(ExtractNodeRule("value", value), "++;"));
	def createKeywordRule(this : Self, keyword : String) => TypeRule(keyword, LeftRule(keyword, RightRule(StripRule(EmptyRule("value")), ";")));
	def createTypeParamsRule(this : Self) => {
		
		typeParam : var=
		
		LazyRule();
		
		symbol : var=
		
		TypeRule("symbol-type", StripRule(SymbolRule(ExtractStringRule("value"))));
		
		extendsRule : var=
		
		TypeRule("extends", StripRule(FirstRule(ExtractStringRule("name"), " extends ", ExtractNodeRule("child", typeParam))));
