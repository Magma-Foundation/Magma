import java.util.List;
import java.util.Optional;
export default class def MultipleError() => {
	private final errors : List<Error_>;
	MultipleError(errors : List<Error_>) : public => this=errors;
	public findMessage() : Optional<String> => Optional.empty();
	public findCauses() : Optional<List<Error_>> => Optional.of(errors);
	public findContext() : Optional<String> => Optional.empty();
	public calculateDepth() : int => 1+errors.stream().mapToInt(Error_.calculateDepth).max().orElse(0);
}