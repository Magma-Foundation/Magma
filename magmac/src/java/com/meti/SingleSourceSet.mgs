extern import { Files } from java.nio.file;
extern import { Path } from java.nio.file;
extern import { HashSet } from java.util;
extern import { Set } from java.util;

export class def SingleSourceSet() => {
	@Override
	def collect() : Set<Path> => {
        var source = source();
        var set = new HashSet<Path>();
        if (Files.exists(source)) set.add(source);
        return set;
    }
}